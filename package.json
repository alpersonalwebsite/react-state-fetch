{
  "name": "react-state-fetch",
  "version": "0.1.0",
  "description": "Simple example of fetching data with React and setting local state with setState, useState and useEffect, and, a custom hook",
  "keywords": [
    "react state",
    "react state hook",
    "useState hook",
    "useEffect hook",
    "custom hook fetch",
    "custom hook state",
    "fetch and state react"
  ],
  "author": "Al Diaz",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^12.1.1",
    "react-dom": "^16.8.6",
    "react-scripts": "3.3.0",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run prettier"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "build": "react-scripts build && cp build/index.html build/200.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.1.0",
    "prettier": "^2.1.2"
  }
}
